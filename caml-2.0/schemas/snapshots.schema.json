{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/caml2/schemas/snapshots.schema.json",
  "title": "CAML 2.0 Snapshots",
  "type": "object",
  "required": [
    "timeline"
  ],
  "additionalProperties": false,
  "properties": {
    "timeline": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "./snapshot_instance.schema.json#/$defs/snapshot"
      }
    }
  }
}